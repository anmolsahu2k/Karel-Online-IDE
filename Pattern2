function main(){
   moveforward();
   moveforward();
   turnlmove();
   moveforward();
   moveforward();
   turnrmove();
   moveforward();
   moveforward();
   turnlmove();
   moveforward();
   moveforward();
   turnrmove();
   moveforward();
   moveforward();
   putBeeper();
}
function moveforward()
{
   putBeeper();
   move();
   move();  
}

function turnlmove()
{
   putBeeper();
   turnLeft();
   move();
   turnLeft();
}
function turnrmove()
{
   putBeeper();
   turnRight();
   move();
   turnRight();
}

